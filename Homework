CREATE TABLE CarBogdan(
`id_car` INT AUTO_INCREMENT PRIMARY KEY,
`marka` VARCHAR(50),
`model` VARCHAR(50),
`data_vupska` DATE
);

INSERT INTO CarBogdan
VALUES(1, "SUZUKI", "SX4", 20/01/2020),
(2,"AUDI", "A4", 11/05/2005),
(3,"AUDI", "A5", 13/07/2003),
(4,"MERSEDES", "S63", 16/06/2018),
(5,"BMW", "X5", 25/03/2005),
(6,"BMW", "M5", 01/11/2010)

CREATE TABLE PolusBogdan(
`id_pol` INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
`date_start` DATE,
`date_finish` DATE,
`id_car` INT
);

ALTER TABLE PolusBogdan
ADD CONSTRAINT fk_id_car
FOREIGN KEY (id_car)
REFERENCES CarBogdan (id_car);

