///

CREATE TABLE PassportBogdan(
`id_ps` INT AUTO_INCREMENT PRIMARY KEY,
`seria` INT,
`number` INT,
`country` VARCHAR(50),
`data` DATE
);

INSERT INTO PassportBogdan
