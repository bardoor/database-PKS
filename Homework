CREATE TABLE HomeBogdan(
`id_hm` INT AUTO_INCREMENT PRIMARY KEY,
`street` VARCHAR(150),
`number` TINYINT,
`data` DATA,
`id_home` INT
);

CREATE TABLE ResidentBogdan(
`id_res` INT AUTO_INCREMENT PRIMARY KEY,
`fio` TINYTEXT,
`data_birth_day` DATE
);

