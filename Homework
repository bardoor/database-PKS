(6, 21267, 12412, "INDIA", 2000-04-02),
(7, 41321, 2441, "AMERICA", 2011-05-01)

CREATE TABLE PeopleBogdan(
`id` INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
`Name` VARCHAR(50),
`Surname` VARCHAR(150),
`years_old` INT,
`id_pas` INT
);

ALTER TABLE PeopleBogdan
ADD CONSTRAINT fk_id_pas
FOREIGN KEY (id_pas)
REFERENCES PassportBogdan (id_ps);

